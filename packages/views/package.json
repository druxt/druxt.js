{
  "name": "druxt-views",
  "version": "0.16.0",
  "description": "Drupal Views components for Druxt with support for filters, pagination and sorting.",
  "keywords": [
    "cms",
    "decoupled",
    "drupal",
    "druxt",
    "druxtjs",
    "headless",
    "jsonapi",
    "json:api",
    "nuxt",
    "nuxtjs",
    "module",
    "storybook",
    "vue",
    "views"
  ],
  "homepage": "https://druxtjs.org",
  "bugs": {
    "url": "https://github.com/druxt/druxt.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/druxt/druxt.js"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/druxt-views.ssr.js",
      "import": "./dist/druxt-views.esm.js"
    },
    "./components/*": "./dist/components/*"
  },
  "main": "dist/druxt-views.ssr.js",
  "module": "dist/druxt-views.esm.js",
  "files": [
    "dist",
    "nuxt",
    "templates"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "deepmerge": "^4.2.2",
    "drupal-jsonapi-params": "^1.1.12",
    "druxt": "^0.13.0",
    "druxt-blocks": "^0.14.0",
    "druxt-entity": "^0.21.0",
    "druxt-router": "^0.23.0",
    "md5": "^2.3.0"
  },
  "optionalDependencies": {
    "vue": "^2.6.14",
    "vuex": "^3.6.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
