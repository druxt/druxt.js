{
  "name": "druxt-menu",
  "version": "0.11.0",
  "description": "Provides a Menu Vue component to be used within a Druxt (DRUpal nuXT) project.",
  "keywords": [
    "menu",
    "menus",
    "drupal",
    "druxt",
    "nuxt"
  ],
  "license": "MIT",
  "author": "Stuart Clark <stuart@realityloop.com> (realityloop.com)",
  "exports": {
    ".": {
      "require": "./dist/druxt-menu.ssr.js",
      "import": "./dist/druxt-menu.esm.js"
    },
    "./components/*": "./dist/components/*",
    "./mixins/*": "./dist/mixins/*"
  },
  "main": "dist/druxt-menu.ssr.js",
  "module": "dist/druxt-menu.esm.js",
  "files": [
    "dist",
    "nuxt"
  ],
  "dependencies": {
    "deepmerge": "^4.2.2",
    "drupal-jsonapi-params": "^1.1.12",
    "druxt": "workspace:*",
    "druxt-blocks": "workspace:*"
  },
  "optionalDependencies": {
    "vue": "^2.6.14",
    "vuex": "^3.6.2"
  }
}
