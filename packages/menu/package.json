{
  "name": "druxt-menu",
  "version": "0.21.0",
  "description": "Drupal Menu and Menu item Druxt components, with support for the JSON:API Menu Items module.",
  "keywords": [
    "cms",
    "decoupled",
    "drupal",
    "druxt",
    "druxtjs",
    "headless",
    "jsonapi",
    "json:api",
    "menu",
    "menus",
    "nuxt",
    "nuxtjs",
    "module",
    "storybook",
    "vue"
  ],
  "homepage": "https://druxtjs.org",
  "bugs": {
    "url": "https://github.com/druxt/druxt.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/druxt/druxt.js"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/druxt-menu.ssr.js",
      "import": "./dist/druxt-menu.esm.js"
    },
    "./components/*": "./dist/components/*"
  },
  "main": "dist/druxt-menu.ssr.js",
  "module": "dist/druxt-menu.esm.js",
  "files": [
    "dist",
    "templates"
  ],
  "dependencies": {
    "deepmerge": "^4.3.1",
    "drupal-jsonapi-params": "^2.3.2",
    "druxt": "^0.24.0",
    "druxt-blocks": "^0.17.2"
  },
  "optionalDependencies": {
    "vue": "^2.7.16",
    "vuex": "^3.6.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
