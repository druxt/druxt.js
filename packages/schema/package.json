{
  "name": "druxt-schema",
  "version": "0.10.2",
  "description": "Drupal Content Entity schema generator for Druxt with support for View and Form displays.",
  "keywords": [
    "cms",
    "decoupled",
    "drupal",
    "druxt",
    "druxtjs",
    "headless",
    "jsonapi",
    "json:api",
    "nuxt",
    "nuxtjs",
    "module",
    "schema"
  ],
  "homepage": "https://druxtjs.org",
  "bugs": {
    "url": "https://github.com/druxt/druxt.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/druxt/druxt.js"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/druxt-schema.ssr.js",
      "import": "./dist/druxt-schema.esm.js"
    },
    "./components/*": "./dist/components/*"
  },
  "main": "dist/druxt-schema.ssr.js",
  "module": "dist/druxt-schema.esm.js",
  "files": [
    "dist",
    "templates"
  ],
  "dependencies": {
    "consola": "^2.15.0",
    "drupal-jsonapi-params": "^1.2.2",
    "druxt": "^0.18.0"
  },
  "optionalDependencies": {
    "vue": "^3.2.31",
    "vuex": "^4.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
