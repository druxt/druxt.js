{
  "name": "druxt-router",
  "version": "0.29.0",
  "description": "Drupal router for Nuxt, powered by the Drupal Decoupled Router module.",
  "keywords": [
    "cms",
    "decoupled",
    "drupal",
    "druxt",
    "druxtjs",
    "headless",
    "jsonapi",
    "json:api",
    "nuxt",
    "nuxtjs",
    "module",
    "router",
    "vue"
  ],
  "homepage": "https://druxtjs.org",
  "bugs": {
    "url": "https://github.com/druxt/druxt.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/druxt/druxt.js"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/druxt-router.ssr.js",
      "import": "./dist/druxt-router.esm.js"
    },
    "./components/*": "./dist/components/*",
    "./nuxt": "./nuxt/index.js"
  },
  "main": "dist/druxt-router.ssr.js",
  "module": "dist/druxt-router.esm.js",
  "files": [
    "dist",
    "nuxt",
    "templates"
  ],
  "dependencies": {
    "druxt": "^0.21.0",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "vue-router": "^3.6.5"
  },
  "peerDependencies": {
    "drupal-jsonapi-params": "*"
  },
  "optionalDependencies": {
    "vue": "^2.7.10",
    "vuex": "^3.6.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
