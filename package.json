{
  "name": "druxt-schema",
  "version": "0.4.0",
  "description": "Entity Form and View mode schemas for Drupal driven frontends.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/druxt/druxt-schema"
  },
  "keywords": [
    "drupal",
    "druxt",
    "schema",
    "entities",
    "field",
    "fields",
    "nuxt"
  ],
  "author": "Stuart Clark <stuart@realityloop.com> (realityloop.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/druxt/druxt-schema/issues"
  },
  "homepage": "https://github.com/druxt/druxt-schema#readme",
  "main": "dist/druxt-schema.ssr.js",
  "module": "dist/druxt-schema.esm.js",
  "unpkg": "dist/druxt-schema.min.js",
  "files": [
    "dist",
    "nuxt"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "build:es": "cross-env NODE_ENV=production rollup -c --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup -c --format cjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup -c --format iife",
    "dev": "nodemon --ext js,vue --exec 'npm run lint && npm run build && npm run test'",
    "docs:dev": "druxt-docgen dev",
    "docs:build": "druxt-docgen build",
    "lint": "eslint --ext .js,.vue src",
    "start": "npm run dev",
    "test": "jest"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*"
    ]
  },
  "dependencies": {
    "simple-oauth2": "^3.4.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-buble": "^0.20.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "consola": "^2.15.0",
    "cross-env": "^6.0.3",
    "druxt-docgen": "^0.2.0",
    "druxt-router": "^0.13.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^25.5.4",
    "jest-junit": "^11.1.0",
    "jest-mock-axios": "^3.2.0",
    "md5": "^2.3.0",
    "minimist": "^1.2.0",
    "nodemon": "^2.0.4",
    "rollup": "^1.26.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.6",
    "vue-template-compiler": "^2.6.11",
    "vue-test-utils": "^1.0.0-beta.11",
    "vuex": "^3.5.1"
  },
  "engines": {
    "node": ">=8"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
